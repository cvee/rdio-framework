// Version Number Elements
// Major:
//   00 - 99
// Minor:
//   00 - 99
// Maintenance:
//   00 - 99
// Stage:
//    0 Development
//    1 Alpha
//    2 Beta
//    3 Release Candidate
//   ..
//    9 Final Release
// Stage Number:
//   00 - 99

// CURRENT_PROJECT_VERSION
// Format:
//   MajorMinorMaintenance.Stage.StageNumber
CURRENT_BUILD_VERSION = 010000000

// CURRENT_RELEASE_VERSION
// Format:
//   Major.Minor.Maintenance
CURRENT_RELEASE_VERSION = 1.0.0
